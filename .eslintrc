{
  "extends": ["airbnb-typescript", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "createDefaultProgram": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "no-plusplus": "off"
  }
}
